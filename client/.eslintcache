 {\n      const commentItem = props.comments.find(\n        (comment) => comment.id === Number(id)\n      );\n      console.log(commentItem)\n      setFormData({\n        comment: commentItem.comment,\n      });\n    };\n    if (props.comments.length) {\n      prefillForm();\n    }\n  }, [props.comments]);\n  console.log(props.comments)\n  \n  const handleChange = (e) => {\n    const { name, value } = e.target;\n    const editData = { [name]: value }\n    setFormData((formData) => {\n      return { ...formData, ...editData }\n      \n    });\n  };\n\n  return (\n    <form\n      onSubmit={(e) => {\n        e.preventDefault();\n        props.handleUpdate(id, formData);\n      }}\n    >\n      <h3>Edit Comment</h3>\n      <div className=\"comment-container\">\n        <div className=\"comment-1\">\n          <label>\n            <input className= 'text'\n              type=\"text\"\n              name=\"comment\"\n              value={comment}\n              onChange={handleChange}\n            />\n          </label>\n          <button className=\"create-button2\">Update</button>\n        </div>\n      </div>\n    </form>\n  );\n}\n","/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/Comments.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/components/Header.jsx",["83","84","85","86","87","88"],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/AboutUs.jsx",["89"],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":23,"column":6,"nodeType":"96","endLine":23,"endColumn":8,"suggestions":"97"},{"ruleId":"94","severity":1,"message":"98","line":17,"column":5,"nodeType":"96","endLine":17,"endColumn":7,"suggestions":"99"},{"ruleId":"94","severity":1,"message":"100","line":25,"column":6,"nodeType":"96","endLine":25,"endColumn":22,"suggestions":"101"},{"ruleId":"102","severity":1,"message":"103","line":20,"column":17,"nodeType":"104","endLine":2