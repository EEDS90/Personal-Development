\n  const handleLogin = async (loginData) => {\n    const userData = await loginUser(loginData);\n    setCurrentUser(userData);\n    history.push('/');\n  }\n\n  const handleRegister = async (registerData) => {\n    const userData = await registerUser(registerData);\n    setCurrentUser(userData);\n    history.push('/');\n  }\n\n  const handleLogout = () => {\n    setCurrentUser(null);\n    localStorage.removeItem('authToken');\n    removeToken();\n    history.push('/');\n  }\n\n  return (\n    <Layout\n      currentUser={currentUser}\n      handleLogout={handleLogout}\n    >\n      <Switch>\n\n        <Route path='/login'>\n          \n          <Login handleLogin={handleLogin} />\n        </Route>\n\n        <Route path='/register'>\n         \n          <Register handleRegister={handleRegister} />\n        </Route>\n\n        <Route path='/'>\n          \n          <MainContainer currentUser={currentUser} />\n        </Route>\n\n      </Switch>\n    </Layout>\n  );\n}\n\nexport default App;","/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/reportWebVitals.js",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/services/auth.js",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/containers/MainContainer.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/layouts/Layout.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/Login.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/Register.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/services/apiConfig.js",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/Questions.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/services/comments.js",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/CommentDetail.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/CommentEdit.js