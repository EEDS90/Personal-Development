port React, { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\n\nexport default function CommentEdit(props) {\n  const [formData, setFormData] = useState({\n    comment: \"\",\n  });\n\n  const {comment} = formData\n  const { id } = useParams();\n\n  useEffect(() => {\n    const prefillForm = () => {\n      const commentItem = props.comments.find(\n        (comment) => comment.id === Number(id)\n      );\n      console.log(commentItem)\n      setFormData({\n        comment: commentItem.comment,\n      });\n    };\n    if (props.comments.length) {\n      prefillForm();\n    }\n  }, [props.comments]);\n  console.log(props.comments)\n  \n  const handleChange = (e) => {\n    const { name, value } = e.target;\n    const editData = { [name]: value }\n    setFormData((formData) => {\n      return { ...formData, ...editData }\n      \n    });\n  };\n\n  return (\n    <form\n      onSubmit={(e) => {\n        e.preventDefault();\n        props.handleUpdate(id, formData);\n      }}\n    >\n      <h3>Edit Comment</h3>\n      <div className=\"comment-container\">\n        <div className=\"comment-1\">\n          <label>\n            <input className= 'text'\n              type=\"text\"\n              name=\"comment\"\n              value={comment}\n              onChange={handleChange}\n            />\n          </label>\n          <button className=\"create-button2\">Update</button>\n        </div>\n      </div>\n    </form>\n  );\n}\n","/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/Comments.jsx",[],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/components/Header.jsx",["88","89","90","91","92","93"],"/Users/edwindiaz/Edwin/Project4/Personal-Development/client/src/screens/AboutUs.jsx",["94"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nimport \"./AboutUs.css\";\n\nexport default function AboutUs() {\n  return (\n    <div className=\"container\">\n      <center>\n     